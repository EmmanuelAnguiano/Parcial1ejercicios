   recorrido(base,hi,mi,si,hf,mf,sf,hinicio,hfinal)
   {
     let bini=this.buscar(base);
     let recorrido=hfinal
     if (bini!=null) 
        temp=bini;
        while((hf-hi>=0)(mf-mi>=0)(sf-si>=0)){
            recorrido+= temp.info() + tiempo 
            temp=temp.siguiente;
        }
   }
